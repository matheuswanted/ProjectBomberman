<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="player">
    <sequence name="up"  start="0" end="2" loop="false" />
    <sequence name="right"  start="9" end="11" loop="false" />
    <sequence name="down"  start="18" end="21" loop="false" />
    <sequence name="left"  start="28" end="31" loop="false" />
</animation>

