<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="player">
    <sequence name="up"  start="0" end="2" loop="false" />
    <sequence name="right" start="3" end="5" loop="false" />
    <sequence name="down" start="6" end="8" loop="false" />
    <sequence name="left" start="9" end="11" loop="false" />
</animation>

