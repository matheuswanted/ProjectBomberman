<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="player">
    <sequence name="down" start="0" end="2" loop="true" />
    <sequence name="left" start="3" end="5" loop="true" />
    <sequence name="up"  start="9" end="11" loop="true" />
    <sequence name="right" start="6" end="8" loop="true" />
    <sequence name="down-stop" start="1" end="1" loop="false" />
    <sequence name="left-stop" start="4" end="4" loop="false" />
    <sequence name="up-stop"  start="10" end="10" loop="false" />
    <sequence name="right-stop" start="7" end="7" loop="false" />
    <sequence name="death" start="12" end="15" loop="false" />
</animation>

