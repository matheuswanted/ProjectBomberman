<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="enemy">
    <sequence name="down" start="0" end="3" loop="true" />
    <sequence name="left" start="8" end="11" loop="true" />
    <sequence name="up"  start="4" end="7" loop="true" />
    <sequence name="right" start="12" end="15" loop="true" />
</animation>

